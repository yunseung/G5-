<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="vertical" >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_marginTop="4dp" >
            
            <TextView
                android:id="@+id/textView1"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:gravity="right|center_vertical"
                android:text="상세주소"
                android:textColor="#555555"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/address_layout_d_et_building_id"
                android:layout_width="248dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:background="@drawable/input01_d"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:textColor="#555555"
                android:enabled="false"
                android:textSize="16sp" />
            
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent" >



                <EditText
                    android:id="@+id/address_layout_d_et_dong_id"
                    android:layout_width="121dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="6dp"
                    android:background="@drawable/input01"
                    android:gravity="right|center_vertical"
                    android:paddingLeft="10dp"
                    android:paddingRight="26dp"
                    android:tag="동"
                    android:textColor="#555555"
                    android:textSize="16sp" >


                </EditText>
                
                                    <TextView
                    android:layout_width="121dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="6dp"
                    android:gravity="right|center_vertical"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:text="동"
                    android:textColor="#555555"
                    android:textSize="16sp" >
                    </TextView>

            </RelativeLayout>

        </LinearLayout>
<LinearLayout
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_marginTop="4dp" >
            
            <TextView
                android:id="@+id/textView11"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:gravity="right|center_vertical"
                android:textColor="#555555"
                android:textSize="16sp" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent" >


                <EditText
                    android:id="@+id/address_layout_d_et_story_id"
                    android:layout_width="121dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/input01"
                    android:gravity="right|center_vertical"
                    android:paddingLeft="10dp"
                    android:paddingRight="26dp"
                    android:tag="층"
                    android:textColor="#555555"
                    android:textSize="16sp" />
                
                <TextView
                    android:layout_width="121dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:gravity="right|center_vertical"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:text="층"
                    android:textColor="#555555"
                    android:textSize="16sp" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent" >

      

                <EditText
                    android:id="@+id/address_layout_d_et_ho_id"
                    android:layout_width="121dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="6dp"
                    android:background="@drawable/input01"
                    android:gravity="right|center_vertical"
                    android:paddingLeft="10dp"
                    android:paddingRight="26dp"
                    android:tag="호"
                    android:textColor="#555555"
                    android:textSize="16sp" />
                <TextView
                    android:layout_width="121dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="6dp"
                    android:gravity="right|center_vertical"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:text="호"
                    android:textColor="#555555"
                    android:textSize="16sp" />

            </RelativeLayout>
             
              <TextView
                  android:id="@+id/textView12"
                  android:layout_width="91dp"
                  android:layout_height="match_parent"
                  android:layout_marginLeft="10dp"
                  android:gravity="right|center_vertical"
                  android:paddingRight="10dp"
                  android:text="상호명"
                  android:textColor="#777777"
                  android:textSize="16sp" />

            <EditText
                android:id="@+id/address_layout_d_et_name_id"
                android:layout_width="248dp"
                android:layout_height="match_parent"
                android:background="@drawable/input01"
                android:gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:textColor="#555555"
                android:textSize="16sp" />

        </LinearLayout>
    </LinearLayout>

</RelativeLayout>

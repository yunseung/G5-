<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <LinearLayout
        android:id="@+id/row_back_id"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/table_list_selector"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/row_id1"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:src="@drawable/check_off" />

        <TextView
            android:id="@+id/row_id2"
            android:layout_width="94dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="10"
            android:textColor="#555555"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/row_id3"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:maxLines="1"
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:text="서울남부MOT"
            android:textColor="#555555"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/row_id4"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:text="박선철"
            android:textColor="#555555"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/row_id5"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="010-5155-9000"
            android:textColor="#555555"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/row_call_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/call_btn_selector" />

    </LinearLayout>

</RelativeLayout>
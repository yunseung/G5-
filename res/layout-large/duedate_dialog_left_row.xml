<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/inventory_request_list_back_id"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@drawable/table_list_selector"
        android:gravity="center_vertical" >

        <ImageView
            android:id="@+id/duedate_dialog_left_row_id1"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:src="@drawable/check_selector"/>

        <TextView
            android:id="@+id/duedate_dialog_left_row_id2"
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="34허1234"
            android:textSize="16sp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#333333" />

        <TextView
            android:id="@+id/duedate_dialog_left_row_id3"
            android:layout_width="162dp"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:textSize="16sp"
            android:text="홍길동"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#333333" />

    </LinearLayout>

</RelativeLayout>
